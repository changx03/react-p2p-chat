{
  "name": "react-webrtc-chat",
  "version": "0.0.0",
  "description": "React WebRTC chat with Socket.io, BootStrap and PeerJS",
  "main": "app.js",
  "scripts": {
    "build-js": "babel src/components --watch --out-file src/Components.js ",
    "start-app": "node app.js",
    "start": "npm run start-app"
  },
  "keywords": [
    "webrtc",
    "react"
  ],
  "babel": {
    "plugins": [
      "syntax-jsx"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "connect-mongo": "^0.8.2",
    "express": "~4.13.3",
    "express-session": "^1.12.1",
    "mongo-single": "0.0.1",
    "mongodb": "^2.0.47",
    "mongoose": "^4.2.3",
    "peer": "~0.2.8",
    "react": "~0.12.2",
    "socket.io": "~1.0.6"
  },
  "devDependencies": {
    "babelify": "^7.0.2",
    "browserify": "^12.0.1",
    "reactify": "^1.1.1"
  }
}
